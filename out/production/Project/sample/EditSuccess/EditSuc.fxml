<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            styleClass="grid"
            fx:controller="sample.Controller"
            prefHeight="400.0" prefWidth="600.0">

    <stylesheets>
        <URL value="@../stye.css" />
    </stylesheets>

    <children>
        <Label id="header" fx:id="header" layoutX="500.0" layoutY="26.0" prefHeight="51.0" prefWidth="356.0" styleClass="header" text="Successfully Edited" textFill="#fff8f8">
            <font>
                <Font name="Berlin Sans FB" size="45.0" />
            </font></Label>

        <Button fx:id="home" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonBack" prefHeight="48.0" prefWidth="130.0" styleClass="home" />

        <Button fx:id="backProduct" layoutX="1148.0" layoutY="14.0" mnemonicParsing="false" onAction="#setBackProduct" prefHeight="48.0" prefWidth="130.0" styleClass="backProduct" />
        <Label layoutX="307.0" layoutY="136.0" prefHeight="516.0" prefWidth="698.0" styleClass="edited" />
    </children>

</AnchorPane>
