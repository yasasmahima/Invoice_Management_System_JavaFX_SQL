<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="702.0" prefWidth="1278.0" styleClass="grid" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

    <stylesheets>
        <URL value="@../stye.css" />
    </stylesheets>

    <children>
        <Label id="header" fx:id="header" layoutX="495.0" layoutY="27.0" prefHeight="51.0" prefWidth="287.0" styleClass="header" text="Delete Product" textFill="#fff8f8">
            <font>
                <Font name="Berlin Sans FB" size="45.0" />
            </font></Label>

        <Button fx:id="home" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonBack" prefHeight="48.0" prefWidth="130.0" styleClass="home" />

        <Button fx:id="backProduct" layoutX="1148.0" layoutY="14.0" mnemonicParsing="false" onAction="#setBackProduct" prefHeight="48.0" prefWidth="130.0" styleClass="backProduct" />
      <Label layoutX="144.0" layoutY="606.0" prefHeight="62.0" prefWidth="990.0" text="Once You Delete a Item You Cannot Restore It.There Fore Please be carefull" textFill="#db6012">
         <font>
            <Font name="Berlin Sans FB" size="31.0" />
         </font>
      </Label>
      <Label layoutX="363.0" layoutY="395.0" prefHeight="204.0" prefWidth="552.0" styleClass="warns" />
      <Label layoutX="361.0" layoutY="193.0" prefHeight="48.0" prefWidth="268.0" text="Enter Product Id    :   " textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="28.0" />
         </font>
      </Label>
      <TextField fx:id="deletedId" layoutX="631.0" layoutY="199.0" prefHeight="36.0" prefWidth="277.0" />
      <Button fx:id="deletedProducts" onAction="#deleteProduct" layoutX="540.0" layoutY="280.0" mnemonicParsing="false" prefHeight="51.0" prefWidth="181.0" styleClass="deleB" />
    </children>
</AnchorPane>
