<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="668.0" prefWidth="1371.0" styleClass="grid" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">


    <stylesheets>
        <URL value="@../stye.css" />
    </stylesheets>

    <children>

    <Label id="header" fx:id="header" layoutX="565.0" layoutY="10.0" prefHeight="51.0" prefWidth="262.0" styleClass="header" text="Add Products" textFill="#fff8f8">
        <font>
            <Font name="Berlin Sans FB" size="45.0" />
        </font></Label>

        <Button fx:id="home" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonBack" prefHeight="48.0" prefWidth="130.0" styleClass="home" />

        <Button fx:id="backProduct" layoutX="1148.0" layoutY="14.0" mnemonicParsing="false" onAction="#setBackProduct" prefHeight="48.0" prefWidth="130.0" styleClass="backProduct" />
      <Label layoutX="319.0" layoutY="149.0" prefHeight="31.0" prefWidth="210.0" text="Product Name : " textFill="#faf9f9">
         <font>
            <Font name="Berlin Sans FB" size="28.0" />
         </font>
      </Label>
      <Label layoutX="318.0" layoutY="227.0" prefHeight="28.0" prefWidth="165.0" text="Description : " textFill="#fff6f6">
         <font>
            <Font name="Berlin Sans FB" size="28.0" />
         </font>
      </Label>
      <Label layoutX="319.0" layoutY="374.0" prefHeight="31.0" prefWidth="210.0" text="Purchase Price : " textFill="#fdfdfd">
         <font>
            <Font name="Berlin Sans FB" size="28.0" />
         </font>
      </Label>
      <Label layoutX="317.0" layoutY="461.0" prefHeight="28.0" prefWidth="166.0" text="Selling Price : " textFill="#f5efef">
         <font>
            <Font name="Berlin Sans FB" size="28.0" />
         </font>
      </Label>
      <Label layoutX="318.0" layoutY="549.0" text="Quantity : " textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="28.0" />
         </font>
      </Label>
      <TextField fx:id="addName" layoutX="565.0" layoutY="149.0" prefHeight="31.0" prefWidth="425.0" />
      <TextField fx:id="addPurchasePrice" layoutX="564.0" layoutY="374.0" prefHeight="31.0" prefWidth="426.0" />
      <TextArea fx:id="addDescription" layoutX="564.0" layoutY="227.0" prefHeight="87.0" prefWidth="426.0" />
      <TextField fx:id="addSellingPrice" layoutX="564.0" layoutY="461.0" prefHeight="31.0" prefWidth="426.0" />
      <TextField fx:id="addQuantity" layoutX="564.0" layoutY="549.0" prefHeight="31.0" prefWidth="426.0" />
      <Button fx:id="addedProducts" onAction="#addProductsDatabase" layoutX="827.0" layoutY="620.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="166.0" styleClass="addData" />

    </children>

